{
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "enum": [
        "ok",
        "error"
      ],
      "describes": "Response status from Air Quality Index"
    },
    "message": {
      "type": "string",
      "describes": "Specifies reason for error response status"
    },
    "data": {
      "type": "object",
      "describes": "Response data from AQI API",
      "properties": {
        "idx": {
          "type": "number",
          "description": "Unique ID for the city monitoring station",
          "permissions": "read",
          "accessModifier": "public"
        },
        "aqi": {
          "type": "number",
          "description": "Real-time air quality information.",
          "permissions": "read",
          "accessModifier": "public"
        },
        "time": {
          "type": "object",
          "description": "Time.",
          "permissions": "read",
          "accessModifier": "public",
          "properties": {
            "v": {
              "type": "number",
              "description": "EPOCH time when API was requested"
            },
            "tz": {
              "type": "string",
              "description": "timezone"
            },
            "s": {
              "type": "string",
              "description": "UTC timestamp"
            }
          }
        },
        "city": {
          "type": "object",
          "description": "City details.",
          "permissions": "read",
          "accessModifier": "public",
          "properties": {
            "name": {
              "type": "string",
              "description": "City name"
            },
            "url": {
              "type": "string",
              "description": "URL for the city in aqicn.org "
            },
            "geo": {
              "type": "array",
              "description": "Coordinates of location",
              "items": {
                "type": "number"
              }
            }
          }
        },
        "iaqi": {
          "type": "object",
          "properties": {
            "co": {
              "type": "object",
              "description": "Carbon Monoxide",
              "properties": {
                "v": {
                  "type": "number"
                }
              }
            },
            "h": {
              "type": "object",
              "description": "Humidity",
              "properties": {
                "v": {
                  "type": "number"
                }
              }
            },
            "p": {
              "type": "object",
              "description": "Pressure",
              "properties": {
                "v": {
                  "type": "number"
                }
              }
            },
            "so2": {
              "type": "object",
              "description": "Sulphur Dioxide",
              "properties": {
                "v": {
                  "type": "number"
                }
              }
            },
            "no2": {
              "type": "object",
              "description": "Nitrogen Dioxide",
              "properties": {
                "v": {
                  "type": "number"
                }
              }
            },
            "w": {
              "type": "object",
              "description": "Carbon Monoxide",
              "properties": {
                "v": {
                  "type": "number"
                }
              }
            },
            "o3": {
              "type": "object",
              "description": "Ozone",
              "properties": {
                "v": {
                  "type": "number"
                }
              }
            },
            "pm25": {
              "type": "object",
              "description": "particulate matter 2.5",
              "properties": {
                "v": {
                  "type": "number"
                }
              }
            },
            "pm10": {
              "type": "object",
              "description": "particulate matter 10",
              "properties": {
                "v": {
                  "type": "number"
                }
              }
            },
            "t": {
              "type": "object",
              "description": "temperature",
              "properties": {
                "v": {
                  "type": "number"
                }
              }
            }
          }
        }
      }
    }
  }
}

